{
    "version": 2,
    "framework": "laravel",
    "builds": [
        {
            "src": "public/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "public/index.php"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "x-powered-by",
                    "value": "Vercel"
                }
            ]
        }
    ]
}
